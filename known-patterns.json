            "event": "SHIP_ENTER",
            "alert": "audio",
            "reaction": "",
            "warning": "",
            "notes": "Works for ALL 200+ ships. Only match SHUDEvent lines to avoid 3x duplicates.",
            "addedBy": "agent",
            "addedDate": "2026-02-14"
        },
